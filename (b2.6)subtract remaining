# Ask user to input ten numbers
numbers = []   # List to store the numbers

for i in range(10):
    num = float(input(f"Enter number {i+1}: "))
    numbers.append(num)
    
# Calculate the result
result = numbers[0]   # Start with the first number
for num in numbers[1:]:   # Subtract the remaining numbers
    result -= num
    
# Print the result
print(f"The result of {numbers[0]} minus all the remaining numbers is:", result)